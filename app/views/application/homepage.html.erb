<h1> Hello, World! </h1>
<div class="well form-horizontal">	
	<%= form_for :username, url: lookup_path do |form| %>
	  
		  <div class="form-group">
		  	<%= form.label :username %>
		  	<%= form.text_field :username  , class: "form-control" %>
		 </div>
		 
		  <div class="form-group">
		  	<%= form.submit 'Submit', class: 'btn btn-primary btn-block' %>
		  </div>
	<% end  %>
</div>